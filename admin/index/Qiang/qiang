<script>
$(function(){
	if($("#vip").val()!=""){
		$(this).addClass("current1").siblings().removeClass("current1");
		var index=$(this).index();
		$(".xxk1 .big").eq(index).show().siblings().hide();
	}
})
</script>
<script>
	function check(val){
		$.post('/index.php/Qiang/status',{id:val},function(date){
			if(date == '成功'){
				var id = val;
				var statu = '已审核';
				sid = "status"+id
				$("#"+sid).html(statu)
			}
			
		})
	}
</script>
